<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="firstapplication.saarsc.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tictactoe"
        android:label="TickTacToe"
        android:logo="@mipmap/tictactoe"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".TicTacToeGame" />
        <activity android:name=".Hangman"></activity>
    </application>

</manifest>